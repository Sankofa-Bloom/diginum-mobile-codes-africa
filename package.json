{
  "name": "diginum",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "setup:momo": "node scripts/setup-momo-sandbox.js",
    "dev": "vite",
    "prebuild": "node scripts/inject-version.js",
    "build": "vite build",
    "build:dev": "node scripts/inject-version.js && vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:backend": "node backend/build.js",

    "netlify-build": "npm run build",
    "start": "node .output/index.js",
    "postinstall": "cd backend && npm install && cd ../netlify/functions && npm install",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "deploy:staging": "npm run build && netlify deploy",
    "deploy:production": "npm run version:patch && git push && git push --tags && npm run build && netlify deploy --prod",
    "deploy:prod:minor": "npm run version:minor && git push && git push --tags && npm run build && netlify deploy --prod",
    "deploy:prod:major": "npm run version:major && git push && git push --tags && npm run build && netlify deploy --prod"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "cacheDirectories": [
    "node_modules",
    "backend/node_modules"
  ],
